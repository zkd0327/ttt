stations = ["동인천", "도원", "제물포", "도화", "주안", "간석", "동암","백운", "부평", "부개", "송내", "중동", "부천", "소사", "역곡", "온수", "오류동", "개봉", "구로", "구일"]
station_to_number = {station: idx for idx, station in enumerate(stations)}

print(station_to_number)

